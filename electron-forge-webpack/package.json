{
  "name": "electron-forge-webpack",
  "version": "6.0.1-beta.63",
  "description": "Webpack plugin for Electron Forge, lets you use Webpack directly in your tooling",
  "repository": "https://github.com/Danzo7/electron-forge-webpack",
  "contributors": [
    {
      "name": "Samuel Attard"
    },
    {
      "name": "danzo7"
    }
  ],
  "main": "dist/cjs/index.js",
  "module": "dist/esm/index.js",
  "types": "dist/types/index.d.js",
  "scripts": {
    "postinstall": "npm run build:cjs && npm run build:esm  && npm run build:types",
    "build:cjs": "node tools/cleanup cjs && tsc -p config/tsconfig.cjs.json",
    "build:esm": "node tools/cleanup esm && tsc -p config/tsconfig.esm.json",
    "build:types": "node tools/cleanup types && tsc -p config/tsconfig.types.json",
    "package": "npm run build && npm pack"
  },
  "keywords": [
    "Electron-webpack"
  ],
  "devDependencies": {
    "@commitlint/cli": "^13.1.0",
    "@commitlint/config-conventional": "^13.1.0",
    "@electron-forge/async-ora": "6.0.0-beta.63",
    "@electron-forge/core": "6.0.0-beta.63",
    "@electron-forge/plugin-base": "6.0.0-beta.63",
    "@electron-forge/shared-types": "6.0.0-beta.63",
    "@electron-forge/web-multi-logger": "6.0.0-beta.63",
    "@malept/cross-spawn-promise": "^2.0.0",
    "@types/debug": "^4.1.7",
    "@types/express-ws": "^3.0.1",
    "@types/fs-extra": "^9.0.13",
    "@types/jest": "^27.0.1",
    "@types/node": "^17.0.8",
    "@typescript-eslint/eslint-plugin": "^4.31.1",
    "@typescript-eslint/parser": "^4.31.1",
    "chai": "^4.3.3",
    "chalk": "^4.0.0",
    "debug": "^4.3.1",
    "electron-packager": "^15.4.0",
    "eslint": "^7.32.0",
    "eslint-config-prettier": "^8.3.0",
    "eslint-plugin-prettier": "^4.0.0",
    "fs-extra": "^10.0.0",
    "global": "^4.3.2",
    "html-webpack-plugin": "^5.3.1",
    "jest": "^27.2.0",
    "mocha": "^9.0.1",
    "node-polyfill-webpack-plugin": "^1.1.4",
    "prettier": "^2.4.0",
    "ts-jest": "^27.0.5",
    "ts-loader": "^9.2.5",
    "tslib": "^2.3.1",
    "typescript": "^4.4.3",
    "webpack": "^5.52.1",
    "webpack-cli": "^4.8.0",
    "webpack-dev-server": "^4.0.0",
    "webpack-merge": "^5.7.3",
    "which": "^2.0.2",
    "xvfb-maybe": "^0.2.1"
  }
}
