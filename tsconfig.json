{ "compilerOptions": {
  "module": "esnext",
  "target": "es5",
  "moduleResolution": "node",
  "jsx": "react",
  "baseUrl": "./",
  "paths": {
    "*": ["node_modules/*"],
    "@data/*": ["./src/rendered/data/*"],
    "@components/*": ["./src/rendered/components/components/*"],
    "@containers/*": ["./src/rendered/components/containers/*"],
    "@assets/*": ["./src/rendered/assets/*"],
    "toSvg/*": ["./src/rendered/assets/svg/*"]
  },
  "noEmit": false, 
  "strict": true,
  "skipLibCheck": true,
  "forceConsistentCasingInFileNames": true,
  "resolveJsonModule": true,
  "allowSyntheticDefaultImports": true,
  "importsNotUsedAsValues": "remove",
  "esModuleInterop": true,
  "noImplicitAny": true,
  "sourceMap": true,
  "outDir": "dist"},
"include": ["src", "@types"]
}
