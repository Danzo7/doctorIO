{ "compilerOptions": {
  "module": "esnext",
  "target": "es5",
  "moduleResolution": "node",
  "jsx": "react",
  "baseUrl": "./",
  "paths": {
    "*": ["node_modules/*"],
    "@components/*": ["src/renderer/components/components/*"],
    "@buttons/*": ["src/renderer/components/components/buttons/*"],
    "@containers/*": ["src/renderer/components/containers/*"],
    "@assets/*": ["src/renderer/assets/*"],
    "toSvg/*": ["src/renderer/assets/svg/*"]
  },
  "noEmit": false, 
  "strict": true,
  "skipLibCheck": true,
  "forceConsistentCasingInFileNames": true,
  "resolveJsonModule": true,
  "allowSyntheticDefaultImports": true,
  "importsNotUsedAsValues": "remove",
  "esModuleInterop": true,
  "noImplicitAny": true,
  "sourceMap": true,
  "outDir": "dist"},
  "skipLibCheck": true,
"include": ["src", "@types"],
"exclude": ["node_modules"]

}
